  jdk 'jdk-17'
 }
 stages {
  stage('Build'){
			steps {
				bat "mvn clean install -DskipTests"
			}
		}
  stage('Sonarqube') {
                environment {
                    scannerHome = tool 'SonarQubeScanner'
                }
                		
 }
}
